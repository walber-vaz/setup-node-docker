  "version": "0.0.1",
  "description": "Setup nodejs DockerFile",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon",
    "husky:prepare": "husky install",
    "lint": "eslint 'src/**' --ext '.js, .ts' --fix && prettier --check 'src/**'",
    "lint:prettier": "prettier --check 'src/**'",
    "test": "jest --passWithNoTests",
